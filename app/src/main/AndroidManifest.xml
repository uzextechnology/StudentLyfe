<?xml version="1.0" encoding="utf-8"?>
<manifest package="uzextechnology.studentlyfe"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:background="@color/backgroundColor"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".EventsActivity"/>
        <activity android:name=".Courses_OverviewActivity"
                  android:noHistory="false"
            />
        <activity android:name=".Courses_EditingActivity"/>
        <activity android:name=".Courses_WholeLetterGradeLimitsActivity"/>
        <activity android:name=".Courses_PlusMinusLetterGradeLimitsActivity">
        </activity>
    </application>

</manifest>