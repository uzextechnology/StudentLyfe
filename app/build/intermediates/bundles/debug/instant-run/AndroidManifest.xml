<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uzextechnology.studentlyfe"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="23" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:background="@color/backgroundColor"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="uzextechnology.studentlyfe.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="uzextechnology.studentlyfe.EventsActivity" />
        <activity
            android:name="uzextechnology.studentlyfe.Courses_OverviewActivity"
            android:noHistory="false" />
        <activity android:name="uzextechnology.studentlyfe.Courses_EditingActivity" />
        <activity android:name="uzextechnology.studentlyfe.Courses_WholeLetterGradeLimitsActivity" />
        <activity android:name="uzextechnology.studentlyfe.Courses_PlusMinusLetterGradeLimitsActivity" >
        </activity>
    </application>

</manifest>